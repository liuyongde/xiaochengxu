/* pages/login/login.wxss */
.message {
  width:100%;
  height:40px;
  background-color: #f4f4f4;
}
.carousel{
  width: 100%;
  background-color: rebeccapurple;
}
.carousel_swiper{
  width: 100%;
  height: 400rpx;
  display: block;
  position: relative;
  background: #f4f4f4;
}
.carouselImg{
  width: 100%;
  height: inherit;
}
.message_img{
  margin:5px;
  width: 20px;
  height: 20px;
}
.message_index{
  width: 96%;
  margin: 6px auto;
  height: 30px;
  background-color: white;
  border-radius: 90px;
  display: flex;
}
.message_content{
  height: 30px;
  line-height: 30px;
  font-size: 0.1em;
}
.pro_content{
  padding: 10px;
}
.pro_info{
  margin:10px 0px;
  height:120px;
  width:100%;
  display: flex;
}
.navigator-hover {
  color:blue;
}
/** 自定义其他点击态样式类 **/
.other-navigator-hover {
  color:red;
}